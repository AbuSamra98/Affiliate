@model Store.Base.Models.PrivacyPolicy

@{
    ViewData["Title"] = "Privacy Policy";
}

<h1>@ViewData["Title"]</h1>
<br />
<input id="getprivacyPolicy" type="text" value="@Model.Description" hidden />
<div id="privacyPolicy"></div>



@section Scripts{
    <script>

        $(document).ready(function () {
            var privacyPolicy = $('#getprivacyPolicy').val();

            $('#privacyPolicy').html($(privacyPolicy));
        });

    </script>
}


