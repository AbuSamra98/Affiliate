@using Store.Base.Utility


    <ul class="list-group">
        @if (User.IsInRole(SD.VendorEndUser))
        {
            <li class="list-group-item"><a asp-action="UpdateVendorProfile">Profile</a></li>
        }
        else if (User.IsInRole(SD.MarketerEndUser))
        {
            <li class="list-group-item"><a asp-action="UpdateMarketerProfile">Profile</a></li>
        }
        <li class="list-group-item"><a id="change-password" asp-action="ChangePassword">Password</a></li>
    </ul>
